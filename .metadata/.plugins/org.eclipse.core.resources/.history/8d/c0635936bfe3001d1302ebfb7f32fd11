package com.demo;

public class A30B40C50_120 {

	public static void main(String[] args) {

		
		String s = "a30b40c50";
		int i,j,k,sum=0;
		
		for(i=0;i<s.length();i++)
		{
			for(j=i+1;j<s.length();j++)
			{
				if(s.charAt(i)>=48&&s.charAt(i)<=56)
				{
					sum+=s.charAt(i)-48;
					System.out.println(sum);
					
				}
			
			}
		}
		System.out.println(sum);
	}

}
